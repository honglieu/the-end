{
  "name": "console-webapp",
  "version": "0.0.0",
  "engines": {
    "node": "^18.13.0 || ^20.9.0",
    "yarn": ">=1.22.4 <2",
    "npm": "Please use yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "ng": "ng",
    "start": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng serve --port=4200 --host=127.0.0.1",
    "prepare-build": "node ./replace.build.js",
    "build": "node ./replace.build.js && node --max-old-space-size=4000 ./node_modules/@angular/cli/bin/ng build",
    "build-live": "node --max-old-space-size=4000 ./node_modules/@angular/cli/bin/ng build --configuration=production",
    "build-produs": "node --max-old-space-size=4000 ./node_modules/@angular/cli/bin/ng build --configuration=produs",
    "build-preprod": "node --max-old-space-size=4000 ./node_modules/@angular/cli/bin/ng build --configuration=preproduction",
    "build-stage": "node --max-old-space-size=4000 ./node_modules/@angular/cli/bin/ng build --configuration=stage",
    "build-attic": "node --max-old-space-size=4000 ./node_modules/@angular/cli/bin/ng build --configuration=attic",
    "build-cellar": "node --max-old-space-size=4000 ./node_modules/@angular/cli/bin/ng build --configuration=cellar",
    "build:trudi-ui": "ng build trudi-ui",
    "watch": "node ./replace.build.js && ng build --watch --configuration development",
    "test": "jest --silent --coverage",
    "storybook": "start-storybook -s src",
    "build-storybook": "build-storybook",
    "lint": "ng lint",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org trudi-b0 --project portal ./dist/console-webapp && sentry-cli sourcemaps upload --release='1721205672338' --org trudi-b0 --project portal ./dist/console-webapp && rm -f ./dist/console-webapp/*.map",
    "sentry:sourcemaps:prod": "sentry-cli sourcemaps inject --org trudi-b0 --project portal-prod ./dist/console-webapp && sentry-cli sourcemaps upload --release='1721205672338' --org trudi-b0 --project portal-prod ./dist/console-webapp && rm -f ./dist/console-webapp/*.map",
    "prepare": "cd ../../ && husky frontend/console-v2/.husky"
  },
  "lint-staged": {
    "*.{ts,js,html}": [
      "eslint --fix --quiet"
    ]
  },
